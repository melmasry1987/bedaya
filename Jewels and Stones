class Solution:
    def numJewelsInStones(self, J, S):
        """
        :type J: str
        :type S: str
        :rtype: int
        """
        
        counter = 0
        stone_length = len(S)
        jewel_length = len(J)
      #  print (stone_length)
     #   print( jewel_length)
        for jewel in range(0,jewel_length):
            for stone in range (0,stone_length):
             #   print(J[jewel]) 
                if J[jewel] == S[stone]:
                    counter = counter +1
                
        return counter
                
                
                
    
